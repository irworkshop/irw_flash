/* hacked up from http://code.google.com/p/csvlib/ **/package org.fenton.loaders{	import flash.net.*	import flash.events.*	public class parsecomma extends URLLoader{		private var FieldSeparator:String	private var RecordsetDelimiter:String                	private var Header:Array	public function parsecomma( request : URLRequest = null )	{                        		FieldSeparator = ',';       	RecordsetDelimiter      = '\n';                              	if( request ) {			trace("tracing request: " + request);         	load( request );		}        this.dataFormat = URLLoaderDataFormat.TEXT        addEventListener( Event.COMPLETE, decode )                        	}		public function decode( event : Event = null ) : void    {		trace("running decode");		var count  : int = 0        var result : Array = new Array ()                        data = data.toString().split( RecordsetDelimiter );        for(  var i : int = 0; i < data.length; i++ )        {			trace("handling row " + i);			result.push( data[ i ] )		}		data = result;	}                } // end of class} // end of package