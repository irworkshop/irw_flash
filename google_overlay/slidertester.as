package{import flash.display.MovieClip;import flash.events.Event;//import org.fenton.loaders.parsecomma;import com.dVyper.utils.Alert;import flash.display.Bitmap;	import org.fenton.sliders.DiscreteHorizontalSlider;[SWF(width="600",height="100", frameRate="24", backgroundColor="0xFFFFFF")]public class slidertester extends MovieClip{			[Embed(source='play.png')]	private var playImg:Class;	private var play1:Bitmap = new playImg ();		// constructor	public function slidertester() {				var sliderLen:Number=500;/*The constructor of the HorizontalSlider class takes two parameters:the length of the slider and the type of the knob. (Possible choices are 'triangle' or 'rectangle'.) The sizeand the coloring of the knob and the slider track can be customized laterusing the methods of the class.*/	play1.height = 48;	play1.width = 48;	play1.x = 5;	play1.y = 7; 	addChild(play1);	var hsSliderRed:DiscreteHorizontalSlider=new DiscreteHorizontalSlider(sliderLen,28, 1981, true);	addChild(hsSliderRed);	hsSliderRed.x=62;	hsSliderRed.y=2;		hsSliderRed.setKnobPos(2008);			hsSliderRed.addEventListener(DiscreteHorizontalSlider.SLIDER_CHANGE, traceChange);						  	function traceChange(e:Event): void {		trace("new slider position: " + String(hsSliderRed.getKnobPos()) );	}	}} // end of class} // end of package