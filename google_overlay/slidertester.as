package{import flash.display.MovieClip;import flash.events.Event;//import org.fenton.loaders.parsecomma;import com.dVyper.utils.Alert;import org.fenton.sliders.DiscreteVerticalSlider;[SWF(width="500",height="300", frameRate="24", backgroundColor="0xFFFFFF")]public class slidertester extends MovieClip{		// constructor	public function slidertester() {	var sliderLen:Number=100;/*The constructor of the HorizontalSlider class takes two parameters:the length of the slider and the type of the knob. (Possible choices are 'triangle' or 'rectangle'.) The sizeand the coloring of the knob and the slider track can be customized laterusing the methods of the class.*/	var hsSliderRed:DiscreteVerticalSlider=new DiscreteVerticalSlider(50,3, true);	addChild(hsSliderRed);	hsSliderRed.x=20;	hsSliderRed.y=30;		hsSliderRed.setKnobPos(25);			hsSliderRed.addEventListener(DiscreteVerticalSlider.SLIDER_CHANGE, traceChange);						  	function traceChange(e:Event): void {		trace("new slider position: " + String(hsSliderRed.getKnobPos()) );	}	}} // end of class} // end of package