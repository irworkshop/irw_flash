// ************************************************************************ //																			//  A basic rollover intended to be updated on mousemove 													//  Hacked together by jacob fenton																						//																			// ************************************************************************ package org.fenton.tooltips{		import flash.display.Sprite;		public class Circle{						public function Circle(dest: Graphics, center_x:Number, center_y:Number, radius:int, fillcolor:uint, fillalpha:Number, outlinewidth:int, outlinecolor:uint, outlinealpha: Number, zoom: Number=1):void {			dest.lineStyle(outlinelinewidth, outlinecolor, outlinealpha);		dest.beginFill(fillcolor, fillalpha);		dest.drawCircle(center_x, center_y, radius);		dest.endFill();		}			} // end of class} // End of package	