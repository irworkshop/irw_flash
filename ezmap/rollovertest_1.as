// Test the rollover classpackage{	import flash.display.Sprite;	import flash.display.Graphics;		import flash.events.*;		// my tooltip	import org.fenton.tooltips.Rollover;	// dVyper's handy alert class	import com.dVyper.utils.Alert;		[SWF(width="620",height="350", frameRate="30", backgroundColor="0xFFFFFF")]		public class rollovertest_1 extends Sprite{			// This is the constructor		public function rollovertest_1() {		init();	}		//private function handlemouseover(x:int, y:int):void {			//	rollover.moverollover(x,y);	//}		private function init():void {			Alert.init(stage);		Alert.show("Swf loaded", {buttons:["OK"]});				var ro:Rollover = new Rollover(stage);						var s: Sprite = new Sprite();		var g: Graphics = s.graphics;				var s1: Sprite = Circle(100, 200, 50, 0xFF0000, 0.8, 3, 0x00FF00, 1); 		s.addChild(s1);		// add sprite to canvas:		addChild(s);										s1.addEventListener(MouseEvent.MOUSE_MOVE, function(e:MouseEvent):void {		// Get the coordinates in stageX/stageY -- the actual sprite is scaled with a zoom factor			ro.moveto(e.stageX, e.stageY)		}, false, 0, false);		}		public function Circle(center_x:Number, center_y:Number, radius:int, fillcolor:uint, fillalpha:Number, outlinewidth:int, outlinecolor:uint, outlinealpha: Number):Sprite {			var s: Sprite = new Sprite();		var g: Graphics = s.graphics;		g.lineStyle(outlinewidth, outlinecolor, outlinealpha);		g.beginFill(fillcolor, fillalpha);		g.drawCircle(center_x, center_y, radius);		g.endFill();		return s;	}	} // end of class	} // End of package